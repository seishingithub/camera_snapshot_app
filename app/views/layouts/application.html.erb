<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">

  <title>CameraSnapshot</title>

  <link rel="stylesheet" type="text/css" href="http://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.1/normalize.min.css">
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>

</head>
<body>
<header>
  <div id="header_container">
    <div id="header_logo">
      <a href="<%= root_path %>"><img src="/images/camera_snapshot_logo_no_filmstrip.png" width="125" alt="Logo"/></a>
    </div>
    <!--<div id="header_phrase">-->
    <!--<p>Digital<br>Camera<br>Reviews<br>Photos<br>& Fun</p>-->
    <!--</div>-->
    <!--end header_logo-->
  </div>
  <!--end header-->
</header>

<div class="colorbar1">&nbsp;</div>

<nav>
  <div id="nav_container">
    <div class="back">
      <div class="button_base b05_3d_roll">
        <div>Home</div>
        <div>Home</div>
        <%= link_to "Home", root_path %>
      </div>
    </div>

    <div class="back">
      <div class="button_base b05_3d_roll">
        <div>Photo Select</div>
        <div>Photo Select</div>
        <%= link_to "Photo Select", photo_select_index_path %>
      </div>
    </div>

    <div class="back">
      <div class="button_base b05_3d_roll">
        <div>eCards</div>
        <div>eCards</div>
        <%= link_to "ECards", new_ecard_path %>
      </div>
    </div>

  </div>
</nav><!--end navigation-->

<div class="colorbar1">&nbsp;</div>

<%= yield %>

</body>
</html>
